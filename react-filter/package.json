{
  "name": "@jet-pack/react-filter",
  "version": "1.0.0",
  "description": "Search/Filter data coming from a react prop",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm ln ../utils",
    "build": "tsc",
    "lint": "./node_modules/.bin/tslint --project tsconfig.json --config tslint.json",
    "test:unit": "nyc mocha src/**/*.unit.test.ts --recursive --require ts-node/register --require source-map-support/register",
    "test:integration": "nyc mocha src/**/*.integration.test.ts --recursive --require ts-node/register --require source-map-support/register",
    "test": "TS_NODE_FILES=true nyc mocha src/**/*.test.ts src/**/**/*.test.ts src/**/*.test.tsx src/**/**/*.test.tsx --recursive --require ts-node/register --require source-map-support/register --require jsdom-global/register --require ./unitTestSetup.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmaximilian/jet-pack.git"
  },
  "keywords": [
    "bmaximilian",
    "fuzzy",
    "search",
    "filter",
    "helper",
    "utils",
    "react",
    "hoc",
    "jet",
    "pack"
  ],
  "author": "Maximilian Beck <contact@maximilianbeck.de> (maximilianbeck.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bmaximilian/jet-pack/issues"
  },
  "browserslist": [
    "last 1 versions",
    "not ie <= 10"
  ],
  "engines": {
    "node": ">= 8.0.0"
  },
  "homepage": "https://github.com/bmaximilian/jet-pack/blob/master/react-filter/README.md",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-spies": "^1.0.0",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/history": "^4.7.2",
    "@types/jsdom": "^12.2.3",
    "@types/lodash": "^4.14.130",
    "@types/mocha": "^5.2.6",
    "@types/qs": "^6.5.3",
    "@types/react": "16.8.12",
    "@types/react-redux": "^7.0.6",
    "@types/recompose": "^0.30.6",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "eslint-plugin-compat": "^3.1.1",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.0",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "source-map-support": "^0.5.12",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-jsdoc-rules": "^0.2.0",
    "tslint-no-circular-imports": "^0.6.2",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.5"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 55,
    "statements": 55,
    "functions": 45,
    "branches": 30,
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "dist",
      ".nyc_output",
      "coverage",
      "node_modules",
      "**/index.*",
      "**/**/*.test.ts",
      "**/**/*.spec.ts",
      "**/**/*.test.tsx",
      "**/**/*.spec.tsx",
      "unitTestSetup.ts"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  },
  "peerDependencies": {
    "@jet-pack/utils": "^1.0.0",
    "react": "^16.8.6",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1"
  },
  "dependencies": {
    "fuse.js": "^3.4.4",
    "history": "^4.9.0",
    "jsum": "^0.1.4",
    "lodash": "^4.17.11",
    "qs": "^6.7.0",
    "recompose": "^0.30.0",
    "rxjs": "^6.5.2"
  }
}
